      <eStructuralFeatures xsi:type="ecore:EReference" name="refinesCharacteristics"
          upperBound="-1" eType="#//characteristic/Characteristic"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="characteristics" unique="false"
          upperBound="-1" eType="#//characteristic/Characteristic" changeable="false"
          volatile="true" transient="true" derived="true">
