      <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
        <details key="characteristicCanOnlyBeCharacterizedWithManifestationsOfNestedCharacteristics"
            value="&#xA;&#x9;&#x9;&#x9;&#x9;self.localManifestations->forAll(it | self.characteristics->exists(it.characteristic))"/>
      </eAnnotations>
      <eOperations name="computeCharacteristic" lowerBound="1" eType="#//characteristic/Characteristic">
        <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
          <details key="body" value="self"/>
        </eAnnotations>
