          <details key="derivation" value="localCharacteristics.characteristic->union(refinesCharacteristics.characteristics)"/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CharacteristicApplicationRule" abstract="true">
