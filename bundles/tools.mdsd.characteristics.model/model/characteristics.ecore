          <eTypeArguments eClassifier="#//manifestation/Manifestation"/>
        </eGenericType>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="StaticManifestation" abstract="true"
